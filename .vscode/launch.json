{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Docker",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "port": 2345,
      "substitutePath": [
        {
          "from": "${workspaceFolder}",
          "to": "/go/src/app"
        },
        {
          "from": "/usr/local/bin/go/pkg",
          "to": "/go/pkg"
        }
      ]
    }
  ]
}
